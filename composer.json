{
    "name": "quetzal-studio/elasticsearch-logger",
    "description": "Elasticsearch logger for Laravel",
    "authors": [
        {
            "name": "Ata",
            "email": "quetzalskeith@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "elasticsearch/elasticsearch": "^8.9"
    },
    "autoload": {
        "psr-4": {
            "QuetzalStudio\\ElasticsearchLogger\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "QuetzalStudio\\ElasticsearchLogger\\ElasticsearchLoggerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
